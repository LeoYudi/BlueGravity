{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/simple-cta",
	"version": "0.1.0",
	"title": "Simple Cta",
	"category": "widgets",
	"icon": "editor-unlink",
	"description": "Example block scaffolded with Create Block tool.",
	"example": {},
	"attributes": {
		"ctaTitle": {
			"type": "string"
		},
		"ctaParagraph": {
			"type": "string"
		},
		"ctaImageURL": {
			"type": "string"
		}
	},
	"supports": {
		"color": {
			"background": true,
			"text": true
		},
		"html": false,
		"typography": {
			"fontSize": true
		}
	},
	"textdomain": "simple-cta",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php"
}